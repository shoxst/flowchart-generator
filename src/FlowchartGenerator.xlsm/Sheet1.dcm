Sub Test()
    Dim blocks As New Collection
    
    Set a = New TerminalBlock
    a.InnerText = "AAA"
    a.IsStart = True
    Set a.PrevBlock = New BlockBase
    a.PrevBlock.Area.Left = 50
    a.PrevBlock.Area.Bottom = 100
    blocks.Add a
    
    Set b = New ProcessBlock
    b.InnerText = "BBB"
    Set b.PrevBlock = a
    blocks.Add b

    Set c = New TerminalBlock
    c.InnerText = "CCC"
    c.IsEnd = True
    Set c.PrevBlock = b
    blocks.Add c

    Dim elm As BlockBase
    For Each elm In blocks
        elm.WriteBlock
    Next
End Sub
