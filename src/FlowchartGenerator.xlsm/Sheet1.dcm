VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "Sheet1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Sub Run()
    Dim left, top, bottom As Single
    left = 50
    bottom = 100
    
    Dim symbols As New Collection
       
    Set a = New Terminal
    a.innerText = "AAA"
    a.isStart = True
    symbols.Add a
    
    Dim list As New Collection
    
    Set d = New Process
    d.innerText = "DDD"
    list.Add d
    
    Set e = New Terminal
    e.innerText = "EEE"
    list.Add e
    
    Set c = New Decision
    c.innerText = "CCC"
    Set c.mainSymbols = list
    symbols.Add c
    
    Set b = New Terminal
    b.innerText = "BBB"
    b.isEnd = True
    symbols.Add b
    
    
    Dim connector As Shape
    Set connector = Nothing
    
    Dim i As Integer
    For i = 1 To symbols.Count
        Set symbols(i).prevConnector = connector
        symbols(i).left = left
        symbols(i).top = bottom
        symbols(i).AddShapes
        Set connector = symbols(i).connector
        bottom = symbols(i).bottom
    Next
        
End Sub


